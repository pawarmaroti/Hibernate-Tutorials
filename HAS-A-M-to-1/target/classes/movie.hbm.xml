<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.maroti.hibernate.beans.Movies">
	<id name="mid"/>
	
	<property name="mname"/>

	<many-to-one name="actor" class="com.maroti.hibernate.beans.Actor">
	<column name="aid"/>
	</many-to-one>
	</class>
	
</hibernate-mapping>
